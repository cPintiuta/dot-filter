{
    "name": "dotkernel/dot-filter",
    "description": "DotKernel filter component customizing and extending laminas-filter",
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "keywords": [
        "dotkernel",
        "filter",
        "factories",
        "container",
        "zf2",
        "zf3",
        "service-manager"
    ],
    "require": {
        "php": "^7.2",
        "laminas/laminas-servicemanager": "^3.3",
        "laminas/laminas-filter": "^2.8",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Filter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Filter\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "2.1-dev"
        }
    }
}
