{
  "name": "dotkernel/dot-filter",
  "description": "DotKernel filter component customizing and extending zend-filter",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "type": "library",
  "license": "MIT",
  "keywords": [
    "dotkernel",
    "filter",
    "factories",
    "container",
    "zf2",
    "zf3",
    "service-manager"
  ],
  "require": {
    "php": "^7.1",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-filter": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "squizlabs/php_codesniffer": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Filter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Filter\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}
